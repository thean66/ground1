3
(
    inlet
    {
        type            patch;
        nFaces          400;
        startFace       104000;
    }
    outlet
    {
        type            patch;
        nFaces          400;
        startFace       104400;
    }
    defaultFaces
    {
        type            empty;
        inGroups        1(empty);
        nFaces          7200;
        startFace       104800;
    }
)

